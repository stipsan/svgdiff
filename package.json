{
  "name": "svgdiff",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "build": "next build",
    "dev": "next",
    "lint": "eslint --ext .tsx,.ts,.jsx,.js . --quiet",
    "now-build": "next build",
    "start": "next start",
    "test": "tsc && yarn lint"
  },
  "dependencies": {
    "@rehooks/component-size": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "@zeit/next-css": "^1.0.2-canary.2",
    "@zeit/next-typescript": "^1.1.1",
    "linaria": "^1.3.3",
    "next": "^8.1.0",
    "normalize.css": "^8.0.1",
    "polished": "^3.4.2",
    "postcss-preset-env": "^6.7.0",
    "react": "^16.10.2",
    "react-ace": "^7.0.2",
    "react-dom": "^16.10.2"
  },
  "devDependencies": {
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.19.1",
    "prettier-package-json": "^2.1.0",
    "typescript": "^3.7.5"
  },
  "engines": {
    "node": "12.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{*.js,*.ts,*.tsx,*.json,*.md,*.yml,.babelrc}": [
      "prettier --write",
      "git add"
    ],
    "**/package.json": [
      "prettier-package-json --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "overrides": [
      {
        "files": ".babelrc",
        "options": {
          "parser": "json"
        }
      }
    ]
  }
}
