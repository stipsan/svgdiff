{
  "name": "svgdiff",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "build": "next build",
    "dev": "next",
    "lint": "eslint --ext .tsx,.ts,.jsx,.js . --quiet",
    "start": "next start",
    "test": "tsc && yarn lint"
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@rehooks/component-size": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "@zeit/next-css": "^1.0.2-canary.2",
    "ace-builds": "^1.4.12",
    "linaria": "^2.0.2",
    "next": "^10.0.1",
    "normalize.css": "^8.0.1",
    "polished": "^4.0.3",
    "postcss-preset-env": "^6.7.0",
    "react": "^17.0.1",
    "react-ace": "^9.0.0",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.7",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.13.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "prettier-package-json": "^2.1.3",
    "typescript": "^4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{*.js,*.ts,*.tsx,*.json,*.md,*.yml,.babelrc}": [
      "prettier --write",
      "git add"
    ],
    "**/package.json": [
      "prettier-package-json --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "overrides": [
      {
        "files": ".babelrc",
        "options": {
          "parser": "json"
        }
      }
    ]
  }
}
