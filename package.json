{
  "name": "svgdiff",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "build": "next build",
    "dev": "next",
    "lint": "eslint --ext .tsx,.ts,.jsx,.js . --quiet",
    "start": "next start",
    "test": "tsc && yarn lint"
  },
  "dependencies": {
    "@rehooks/component-size": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "@zeit/next-css": "^1.0.2-canary.2",
    "linaria": "^1.3.3",
    "next": "^9.0.0",
    "normalize.css": "^8.0.1",
    "polished": "^3.6.7",
    "postcss-preset-env": "^6.7.0",
    "react": "^16.14.0",
    "react-ace": "^7.0.2",
    "react-dom": "^16.14.0"
  },
  "devDependencies": {
    "@types/node": "^14.11.8",
    "@types/react": "^16.9.51",
    "@types/react-dom": "^16.9.8",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^3.1.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "prettier-package-json": "^2.1.3",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": "12.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{*.js,*.ts,*.tsx,*.json,*.md,*.yml,.babelrc}": [
      "prettier --write",
      "git add"
    ],
    "**/package.json": [
      "prettier-package-json --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "overrides": [
      {
        "files": ".babelrc",
        "options": {
          "parser": "json"
        }
      }
    ]
  }
}
