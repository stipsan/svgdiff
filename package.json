{
  "name": "svgdiff",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "build": "next build",
    "dev": "next",
    "lint": "eslint --ext .tsx,.ts,.jsx,.js . --quiet",
    "now-build": "next build",
    "start": "next start",
    "test": "tsc && yarn lint"
  },
  "dependencies": {
    "@rehooks/component-size": "1.0.2",
    "@typescript-eslint/eslint-plugin": "1.1.0",
    "@typescript-eslint/parser": "1.1.0",
    "@zeit/next-css": "1.0.2-canary.2",
    "@zeit/next-typescript": "1.1.1",
    "linaria": "1.0.0-beta.13",
    "next": "8.0.0-canary.17",
    "normalize.css": "8.0.1",
    "polished": "2.3.3",
    "postcss-preset-env": "6.5.0",
    "react": "16.8.0-alpha.1",
    "react-ace": "6.3.2",
    "react-dom": "16.8.0-alpha.1"
  },
  "devDependencies": {
    "@types/next": "7.0.7",
    "@types/react": "16.7.22",
    "@types/react-dom": "16.0.11",
    "babel-eslint": "9.0.0",
    "eslint": "5.12.1",
    "eslint-config-react-app": "3.0.6",
    "eslint-plugin-flowtype": "2.50.3",
    "eslint-plugin-import": "2.15.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "0.0.0",
    "husky": "1.3.1",
    "lint-staged": "8.1.1",
    "prettier": "1.16.3",
    "prettier-package-json": "2.0.1",
    "typescript": "3.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{*.js,*.ts,*.tsx,*.json,*.md,*.yml,.babelrc}": [
      "prettier --write",
      "git add"
    ],
    "**/package.json": [
      "prettier-package-json --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "overrides": [
      {
        "files": ".babelrc",
        "options": {
          "parser": "json"
        }
      }
    ]
  }
}
