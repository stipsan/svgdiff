{
  "name": "svgdiff",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "build": "next build",
    "dev": "next",
    "lint": "eslint --ext .tsx,.ts,.jsx,.js . --quiet",
    "now-build": "next build",
    "start": "next start",
    "test": "tsc && yarn lint"
  },
  "dependencies": {
    "@rehooks/component-size": "1.0.2",
    "@typescript-eslint/eslint-plugin": "1.10.2",
    "@typescript-eslint/parser": "1.10.2",
    "@zeit/next-css": "1.0.2-canary.2",
    "@zeit/next-typescript": "1.1.1",
    "linaria": "1.3.1",
    "next": "8.1.0",
    "normalize.css": "8.0.1",
    "polished": "3.4.0",
    "postcss-preset-env": "6.6.0",
    "react": "16.8.6",
    "react-ace": "7.0.2",
    "react-dom": "16.8.6"
  },
  "devDependencies": {
    "@types/next": "8.0.5",
    "@types/react": "16.8.19",
    "@types/react-dom": "16.8.4",
    "babel-eslint": "10.0.2",
    "eslint": "5.16.0",
    "eslint-config-react-app": "4.0.1",
    "eslint-plugin-flowtype": "3.10.3",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.13.0",
    "eslint-plugin-react-hooks": "1.6.0",
    "husky": "2.4.1",
    "lint-staged": "8.2.1",
    "prettier": "1.18.2",
    "prettier-package-json": "2.1.0",
    "typescript": "3.5.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{*.js,*.ts,*.tsx,*.json,*.md,*.yml,.babelrc}": [
      "prettier --write",
      "git add"
    ],
    "**/package.json": [
      "prettier-package-json --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "overrides": [
      {
        "files": ".babelrc",
        "options": {
          "parser": "json"
        }
      }
    ]
  }
}
